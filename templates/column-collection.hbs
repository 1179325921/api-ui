<div class="column-header">Collection</div>
<ul class="column-body">
  <li>
    <div class="section-header">&#9660;Links</div>
    {{>column-links.hbs}}
  </li>

  <li>
    <div class="section-header">&#9660;Data</div>
    <ul class="collection-data">
    {{#each data}}
      <li class="row" data-id="{{this.id}}" data-self="{{this.links.self}}">{{displayName this}}</li>
    {{else}}
      <div class="muted"><i>None</i></div>
    {{/each}}
    </ul>
  </li>
</ul>
