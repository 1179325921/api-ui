{{#json-collapsible
  open='{'
  close='}'
  length=length
  initExpanded=initExpanded
  collapsible=collapsible
  as |block|
}}
  {{#if (eq block "summary")}}
    {{#if (and model.type model.id)}}
      {{model.type}}: {{model.id}}
    {{else}}
      {{length}} keys
    {{/if}}
  {{else}}
    {{#each-in model as |k v|}}
      <div class="obj-field">
        <span class="obj-key">{{k}}</span>
        <span class="obj-syntax separator">:</span>
        <span class="obj-value no-inline-monospace">
          {{#if hasBlock}}
            {{yield k v}}
          {{else if (and (eq (get componentForKey k) 'json-value') (eq (base-type-of v) 'string'))}}
            <span class="string">&quot;{{v}}&quot;</span>
          {{else if (eq v null)}}
            <span class="null">null</span>
          {{else}}
            {{component (get componentForKey k) key=k model=v parent=model schemas=schemas}}
          {{/if}}
        </span>
        <span class="obj-value no-inline-monospace">
        </span>
        <span class="obj-syntax comma">,</span>
      </div>
    {{/each-in}}
  {{/if}}
{{/json-collapsible}}

