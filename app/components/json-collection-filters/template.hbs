{{#json-collapsible
  open='{'
  close='}'
  length=length
  initExpanded=false
  as |block|
}}
  {{#if (eq block "summary")}}
    {{length}} keys
  {{else}}
    {{#each-in model as |k v|}}
      <div class="obj-field">
        <span class="obj-key">{{k}}</span>
        <span class="obj-syntax separator">:</span>
        <span class="obj-value no-inline-monospace">
          {{#if hasBlock}}
            {{yield k v}}
          {{else}}
            {{json-value model=v initExpanded=true}}
          {{/if}}
        </span>
        <span class="obj-syntax comma">,</span>
      </div>
    {{/each-in}}
  {{/if}}
{{/json-collapsible}}

